<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Drive Forward Racecar">

	<executable package_path="racecar_flexbe_behaviors.drive_forward_racecar_sm" class="DriveForwardRacecarSM" />
	<tagstring>Drive</tagstring>
	<author>Me</author>
	<date>Thu Oct 31 2019</date>
	<description>
		Enable us to drive forward
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="my_speed" default="0.4" label="my_speed" hint="Sets the my_speed">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="my_travel_dist" default="4" label="my_travel_dist" hint="Sets the my_travel_dist">
			<min value="0" />
			<max value="10" />
		</param>

		<param type="numeric" name="my_obstacle_dist" default="1" label="my_obstacle_dist" hint="Sets the my_obstacle_dist">
			<min value="0" />
			<max value="5" />
		</param>

	</params>


</behavior>